    1300003 => SkillId::PA_FA
    1301004 => SkillId::SPEAR_BOOSTER
    1301005 => SkillId::PA_BOOSTER
    1301006 => SkillId::IRON_WILL
    1301007 => SkillId::HYPER_BODY

