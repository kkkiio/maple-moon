    1110001 => SkillId::SHIELD_MASTERY
    1111003 => SkillId::SWORD_PANIC
    1111004 => SkillId::AXE_PANIC
    1111005 => SkillId::SWORD_COMA
    1111006 => SkillId::AXE_COMA
    1111008 => SkillId::SHOUT

    // Hero
